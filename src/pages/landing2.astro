    <!-- HLS.js Script -->
    <script src="https://cdn.jsdelivr.net/npm/hls.js@latest"></script>
    <script>
        let hls = null;
        let videoLoaded = false;
        
        function playVideo() {
            const video = document.getElementById('main-video');
            const poster = document.getElementById('video-poster');
            
            // Mostrar el video
            video.style.display = 'block';
            poster.classList.add('hidden');
            
            // Reiniciar el video desde el principio
            video.currentTime = 0;
            
            // Inicializar HLS
            if (Hls.isSupported()) {
                if (hls) {
                    hls.destroy();
                }
                
                hls = new Hls({
                    debug: false,
                    enableWorker: true,
                    lowLatencyMode: true
                });
                
                hls.loadSource('https://vz-cddd003b-54d.b-cdn.net/945e327c-2058-48e5-8ec2-138c14c13d55/playlist.m3u8');
                hls.attachMedia(video);
                
                hls.on(Hls.Events.MANIFEST_PARSED, function() {
                    console.log('HLS manifest parsed, video ready to play');
                    video.currentTime = 0; // Asegurar que empiece desde el principio
                    video.play().catch(e => {
                        console.log('Autoplay prevented:', e);
                        video.controls = true;
                    });
                });
                
                hls.on(Hls.Events.ERROR, function(event, data) {
                    console.error('HLS Error:', data);
                    if (data.fatal) {
                        switch(data.type) {
                            case Hls.ErrorTypes.NETWORK_ERROR:
                                console.log('Fatal network error, trying to recover...');
                                hls.startLoad();
                                break;
                            case Hls.ErrorTypes.MEDIA_ERROR:
                                console.log('Fatal media error, trying to recover...');
                                hls.recoverMediaError();
                                break;
                            default:
                                console.log('Fatal error, destroying HLS...');
                                hls.destroy();
                                break;
                        }
                    }
                });
                
            } else if (video.canPlayType('application/vnd.apple.mpegurl')) {
                // Safari nativo
                video.src = 'https://vz-cddd003b-54d.b-cdn.net/945e327c-2058-48e5-8ec2-138c14c13d55/playlist.m3u8';
                video.currentTime = 0; // Asegurar que empiece desde el principio
                video.play().catch(e => {
                    console.log('Autoplay prevented:', e);
                    video.controls = true;
                });
            } else {
                console.error('HLS not supported');
                alert('Tu navegador no soporta este tipo de video. Por favor usa Chrome, Firefox o Safari.');
            }
        }
        
        document.addEventListener('DOMContentLoaded', function() {
            console.log('Landing2 page loaded');
            
            // Agregar evento de click a toda la superficie del poster
            const poster = document.getElementById('video-poster');
            if (poster) {
                poster.addEventListener('click', playVideo);
            }
        });
    </script>
    
</body>
